//Faça um programa que cria um vetor com 5 elementos inteiros, lê 5 números do teclado, armazena os números no vetor e imprime o vetor na ordem inversa.

#include<iostream>
using namespace std;


int main(){
	int valor = 0;
	int *vetor = new int [5];
	
	for(int i = 0; i < 5; i++){
		cout << "Valor " << i+1 << ": "; cin >> vetor[i];
	}
	for(int i = 5 - 1; i >= 0; i--){
		cout << vetor[i] << endl;
	}
	
	return 0;
}
